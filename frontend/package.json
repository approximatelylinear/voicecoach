{
  "name": "voicecoach",
  "version": "0.0.1",
  "description": "Interactive voice coach",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=development node webpack.devserver.js",
    "build:sandbox": "npm run test && NODE_ENV=production webpack -p --config ./webpack.config.prod.js --progress --colors --profile --json > stats.json",
    "test": "BABEL_DISABLE_CACHE=1 NODE_ENV=test node --harmony_proxies node_modules/.bin/jest --coverage",
    "snapshot:update": "node_modules/.bin/jest -u",
    "lint": "eslint app/ --ext .js --ext .jsx; exit 0",
    "g": "plop --plopfile config/generators/index.js",
    "g:component": "plop --plopfile config/generators/index.js component",
    "g:container": "plop --plopfile config/generators/index.js container",
    "g:page": "plop --plopfile config/generators/index.js page"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com:approximatelylinear/voicecoach.git"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules",
      "shared"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/config/testing/__mocks__/jestSetupHelper.js",
    "moduleNameMapper": {
      "\\.(css|scss)$": "<rootDir>/config/testing/__mocks__/styleMock.js",
      "\\.(gif|ttf|eot|svg)$": "<rootDir>/config/testing/__mocks__/fileMock.js"
    },
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "globals": {
      "API_BASE_URL": "jest-testing-url",
      "__DEV__": true
    }
  },
  "jest-webpack-alias": {
    "configFile": "./webpack.config.dev.js"
  },
  "author": "mjr.berends@gmail.com",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^6.7.6",
    "axios-mock-adapter": "^1.7.1",
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.3.2",
    "babel-plugin-webpack-alias": "^2.1.2",
    "babel-plugin-webpack-loaders": "^0.9.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-register": "^6.23.0",
    "babel-runtime": "^6.23.0",
    "bootstrap-sass": "^3.3.7",
    "css-loader": "^0.26.2",
    "eslint": "^3.16.1",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-loader": "^1.6.3",
    "eslint-plugin-graphql": "^0.6.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.0",
    "expect-enzyme": "^0.13.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.10.1",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "^2.28.0",
    "node-sass": "^4.5.0",
    "null-loader": "^0.1.1",
    "plop": "^1.7.4",
    "postcss-loader": "^1.3.2",
    "precss": "^1.4.0",
    "redux-devtools": "^3.3.2",
    "redux-mock-store": "^1.2.2",
    "resolve-url-loader": "^2.0.0",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.2",
    "style-loader": "^0.13.2",
    "url-loader": "^0.5.8",
    "webpack": "^2.2.1",
    "webpack-chunk-hash": "^0.4.0",
    "webpack-dev-server": "^2.4.1",
    "webpack-merge": "^4.1.0"
  },
  "dependencies": {
    "axios": "^0.15.3",
    "babel-jest": "^19.0.0",
    "enzyme": "^2.7.1",
    "enzyme-to-json": "^1.5.0",
    "eslint-import-resolver-webpack": "^0.8.1",
    "expect": "^1.20.2",
    "expect-jsx": "^3.0.0",
    "history": "^4.5.1",
    "immutability-helper": "^2.1.2",
    "immutable": "^3.8.1",
    "jest": "^19.0.2",
    "jest-cli": "^19.0.2",
    "jest-webpack-alias": "^3.3.3",
    "json-loader": "^0.5.4",
    "moment": "^2.17.1",
    "purifycss-webpack-plugin": "^2.0.3",
    "react": "^15.5.4",
    "react-addons-test-utils": "^15.4.2",
    "react-addons-update": "^15.4.2",
    "react-bootstrap": "^0.30.7",
    "react-datepicker": "^0.41.1",
    "react-dom": "^15.5.4",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-redux": "^5.0.3",
    "react-router": "^3.0.2",
    "react-router-bootstrap": "^0.23.1",
    "react-router-redux": "^4.0.8",
    "react-slick": "^0.15",
    "redux": "^3.6.0",
    "redux-auth-wrapper": "^1.0.0",
    "redux-logger": "^2.8.1",
    "redux-thunk": "^2.2.0",
    "reselect": "^2.5.4",
    "sinon": "^1.17.7"
  }
}
