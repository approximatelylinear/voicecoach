{
  "presets": [
    "es2015",
    "react",
    "stage-0"
  ],
  "env": {
    "development": {
      "plugins": [
        "react-hot-loader/babel"
      ]
    },
    "production": {
      "plugins": [
        [
          "babel-plugin-webpack-alias",
          {
            "config": "./webpack.config.prod.js"
          }
        ]
      ]
    },
    "test": {
      "plugins": [
        [ "babel-plugin-webpack-alias", { "config": "webpack.config.dev.js" } ]
      ]
    }
  }
}
